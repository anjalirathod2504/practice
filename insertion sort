def insertionSort(a):

    for step in range(1, len(a)):
        key = a[step]
        j = step - 1
        
       
        while j >= 0 and key < a[j]:
            a[j + 1] = a[j]
            j = j - 1
        
        
        a[j + 1] = key


num = [9, 5, 1, 4, 3]
insertionSort(num)
print('insertion sort: ')
print(num)
